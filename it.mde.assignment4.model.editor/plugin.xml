<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated webApplication -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="it.mde.assignment4.model.webApplication.presentation.WebApplicationModelWizardID"
            name="%_UI_WebApplicationModelWizard_label"
            class="it.mde.assignment4.model.webApplication.presentation.WebApplicationModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/WebApplicationModelFile.gif">
         <description>%_UI_WebApplicationModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated webApplication -->
      <editor
            id="it.mde.assignment4.model.webApplication.presentation.WebApplicationEditorID"
            name="%_UI_WebApplicationEditor_label"
            icon="icons/full/obj16/WebApplicationModelFile.gif"
            extensions="webapplication"
            class="it.mde.assignment4.model.webApplication.presentation.WebApplicationEditor"
            contributorClass="it.mde.assignment4.model.webApplication.presentation.WebApplicationActionBarContributor">
      </editor>
   </extension>

</plugin>
